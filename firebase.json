{
  "hosting": [
    {
      "site": "drop-it",
      "public": ".out",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      
      "cleanUrls": true,
      "trailingSlash": false,
      "rewrites": [
        {
          "source": "/",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
